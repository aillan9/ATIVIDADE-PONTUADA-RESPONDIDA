programa {
  funcao inicio() {
    // Organizando váriaveis
    inteiro a, b, c

    // Solicitando váriaveis
    escreva("Digite o primeiro número? ")
    leia(a)
     escreva("Digite o segundo número? ")
    leia(b)



   // Calculando
    se(a == b){
    c = a + b 
    escreva("O resultado foi: " + c)
    }

    senao{
   c = a * b
   escreva("O resultado foi: " + c)
   }



  }
}